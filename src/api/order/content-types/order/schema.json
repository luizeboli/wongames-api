{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "games": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game.game"
    },
    "total_in_cents": {
      "type": "biginteger",
      "required": true
    },
    "payment_intent_id": {
      "type": "string"
    },
    "card_brand": {
      "type": "string"
    },
    "card_last4": {
      "type": "string"
    }
  }
}
